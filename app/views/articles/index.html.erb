<h1><%= link_to 'Add Article' , new_article_path %></h1>

<% @articles.each do |article|%>
<h2>
<%=  link_to article.title, article_path(article)%>
</h2>
<p><%=  article.title %></p>
<p><%=article.content%></p>

<%= link_to 'Update' , edit_article_path(article) %>
<%= link_to 'Delete' , article_path(article), method: :delete %>

<% end %>
